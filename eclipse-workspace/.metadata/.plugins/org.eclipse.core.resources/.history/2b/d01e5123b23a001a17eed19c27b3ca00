	String Renk; 
	
	int year; 
	
	araba(){				 //abstrac oluþturuk. 
		
	}
	
	 araba(String m, String mo, String r, int y) //Parametreli  Yapýcý(abstrack) oluþturduk.
	{
		
		Marka = m; 
		
		Model =  mo; 
		
		Renk = r; 
		
